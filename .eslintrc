{
    "extends": [
      "defaults",
      "prettier"
    ],
    "rules": {
      "radix": 0,
      "no-prototype-builtins": 0,
      "no-return-await": 0,
      "no-empty": 0,
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5",
          "singleQuote": true,
          "printWidth": 100
        }
      ]
    },
    "plugins": [
      "prettier"
    ],
    "globals": {
      "window": true,
      "tron": true
    },
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "es6": true,
      "node": true
    }
  }